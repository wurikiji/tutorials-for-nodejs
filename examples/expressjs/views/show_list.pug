doctype html
html
  head
    meta(charset='utf-8')
  body
    h1 This is for you
    ul
      each list in _list
        li
          a(href='/list/' + list)= list
    p
      - if (_title != null)
        h3 Description of 
          = _title
        = _desc
    form(action='/form_add' method='get')
      p
        input(type='submit' value="Add list")
    
